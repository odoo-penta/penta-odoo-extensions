<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_stock_move_line_detailed_operation_list_inherit_import" model="ir.ui.view">
    <field name="name">stock.move.line.detailed.ops.inherit.import</field>
    <field name="model">stock.move.line</field>
    <field name="inherit_id" ref="stock.view_stock_move_line_detailed_operation_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='product_id']" position="after">
        <field name="id_import"/>
      </xpath>
    </field>
  </record>
    <record id="view_account_move_line_tree_inherit_import" model="ir.ui.view">
    <field name="name">account.move.line.tree.inherit.import</field>
    <field name="model">account.move.line</field>
    <field name="inherit_id" ref="account.view_move_line_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='name']" position="after">
        <field name="id_import"/>
      </xpath>
    </field>
  </record>
</odoo>
