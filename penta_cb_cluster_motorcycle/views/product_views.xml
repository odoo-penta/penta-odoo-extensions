<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="product_attribute_view_form_cluster" model="ir.ui.view">
        <field name="name">product.attribute.form.cluster</field>
        <field name="model">product.attribute</field>
        <field name="inherit_id" ref="product.product_attribute_view_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="hide_pdf"/>
            </field>
        </field>
    </record>
    <record id="product_template_product_form_inherit" model="ir.ui.view">
        <field name="name">product.template.product.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="import_module.view_product_template_import_tab"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='sri_subcategory']" position="after">
                    <field name="is_ensabled"/>
            </xpath>
        </field>
    </record>
</odoo>