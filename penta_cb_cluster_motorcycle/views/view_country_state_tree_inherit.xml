<odoo>
  <record id="view_state_form_inherit_penta" model="ir.ui.view">
    <field name="name">view.state.form.inherit.penta</field>
    <field name="model">res.country.state</field>
    <field name="inherit_id" ref="base.view_country_state_tree"/>
    <field name="arch" type="xml">
      <xpath expr="//field[@name='code']" position="after">
        <field name="l10n_ec_penta_code_state"/>
      </xpath>
    </field>
  </record>
</odoo>
