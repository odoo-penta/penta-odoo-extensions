<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_template_product_form_inherit_penta" model="ir.ui.view">
        <field name="name">product.template.product.form.inherit.penta</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="import_module.view_product_template_import_tab"/>
        <field name="arch" type="xml">
            <xpath expr="//group[2]" position="after">
                <group id="manufacture" string="Manufacture">
                    <field name="is_raw_material" widget="boolean_toggle"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>